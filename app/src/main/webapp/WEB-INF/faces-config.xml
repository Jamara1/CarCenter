<faces-config version="2.3"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd">

    <navigation-rule>
        <from-view-id>*</from-view-id>
        
        <!-- ::::: Routes ::::: -->
        <!-- Home -->
        <navigation-case>
            <from-action>#{nav.showHome}</from-action>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        
        <!-- People -->
        <navigation-case>
            <from-action>#{nav.showDocument}</from-action>
            <from-outcome>documents</from-outcome>
            <to-view-id>/pages/document/document-type-index.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-action>#{nav.showClient}</from-action>
            <from-outcome>clients</from-outcome>
            <to-view-id>/pages/client/client-index.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-action>#{nav.showMechanic}</from-action>
            <from-outcome>mechanics</from-outcome>
            <to-view-id>/pages/mechanic/mechanic-index.xhtml</to-view-id>
        </navigation-case>
        
        <!-- Cars -->
        <navigation-case>
            <from-action>#{nav.showCar}</from-action>
            <from-outcome>cars</from-outcome>
            <to-view-id>/pages/car/car-index.xhtml</to-view-id>
        </navigation-case>
        
        <!-- Sites -->
        <navigation-case>
            <from-action>#{nav.showSite}</from-action>
            <from-outcome>sites</from-outcome>
            <to-view-id>/pages/site/site-index.xhtml</to-view-id>
        </navigation-case>
        
        <!-- Replacements -->
        <navigation-case>
            <from-action>#{nav.showReplacement}</from-action>
            <from-outcome>replacements</from-outcome>
            <to-view-id>/pages/replacement/replacement-index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <!-- ::::: End routes ::::: -->
    
    <!-- ::::: Beans ::::: -->
    <!-- Document type -->
    <managed-bean>
        <managed-bean-name>documentTypeModel</managed-bean-name>
        <managed-bean-class>com.carcenter.app.models.DocumentTypeModel</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>documentTypeController</managed-bean-name>
        <managed-bean-class>com.carcenter.app.controllers.DocumentTypeController</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <!-- Client -->
